{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_powerline_symbol": "\ue0b6",
          "powerline_symbol": "\ue0b0 ",
          "template": " {{.Icon}} ",
          "foreground": "#ffffff",
          "background": "#333333",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\uf007 {{.UserName}}@{{.HostName}} ",
          "foreground": "#ffffff",
          "background": "#555555",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\uf489  {{.Name}} ",
          "foreground": "#ffffff",
          "background": "#777777",
          "type": "shell",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\uf413 {{.Path}} ",
          "foreground": "#ffffff",
          "background": "#007ACC",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_push_status": true,
            "branch_icon": " \udb81\ude2c "
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }} {{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}",
          "foreground": "#193549",
          "background": "#00C853",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFC107{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue718 {{.Full}}",
          "foreground": "#ffffff",
          "background": "#68A063",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue235 {{ if .Venv }}{{ .Venv }} {{ end }}{{.Full}}",
          "foreground": "#ffffff",
          "background": "#306998",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue626 {{.Full}}",
          "foreground": "#ffffff",
          "background": "#00ADD8",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue738 {{.Full}}",
          "foreground": "#ffffff",
          "background": "#f89820",
          "type": "java",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue21e {{.Full}}",
          "foreground": "#ffffff",
          "background": "#CC342D",
          "type": "ruby",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue608 {{.Full}}",
          "foreground": "#ffffff",
          "background": "#777BB4",
          "type": "php",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue7a8 {{.Full}}",
          "foreground": "#000000",
          "background": "#DEA584",
          "type": "rust",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b0 ",
          "template": "\uf81a {{.Full}}",
          "foreground": "#ffffff",
          "background": "#68217A",
          "type": "csharp",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "#ffffff",
          "background": "#33DD33",
          "type": "exit",
          "style": "powerline",
          "background_templates": ["{{ if gt .Code 0 }}#D32F2F{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "threshold": 500,
            "always_enabled": true
          },
          "template": "\uf252 {{ .FormattedMs }} ",
          "foreground": "#888888",
          "background": "transparent",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf00c ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": ""
          },
          "template": "{{ if eq \"Charging\" .State.String }}AC {{ end }}{{ .Icon }}{{ .Percentage }}% ",
          "foreground": "#888888",
          "background": "transparent",
          "type": "battery",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Mon Jan 2, 2006 \uf017 03:04 PM MST"
          },
          "template": "\uf073 {{ .CurrentDate | date .Format }} ",
          "foreground": "#888888",
          "background": "transparent",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf0e7",
          "foreground": "#D32F2F",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚ùØ ",
          "foreground": "#00C853",
          "type": "text",
          "style": "plain",
          "foreground_templates": ["{{ if .Root }}#D32F2F{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}

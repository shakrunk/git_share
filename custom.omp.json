{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#333333",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{.Icon}} "
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#555555",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\uf007 {{.UserName}}@{{.HostName}} "
        },
        {
          "type": "shell",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#777777",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue795 {{.Name}} "
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#007ACC",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "style": "full"
          },
          "template": "\uf413 {{.Path}} "
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#193549",
          "background": "#00C853",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFC107{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#68A063",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue718 {{.Full}}"
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#306998",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue235 {{ if .Venv }}{{ .Venv }} {{ end }}{{.Full}}"
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00ADD8",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue626 {{.Full}}"
        },
        {
          "type": "java",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#f89820",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue738 {{.Full}}"
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#CC342D",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue21e {{.Full}}"
        },
        {
          "type": "php",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#777BB4",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue608 {{.Full}}"
        },
        {
          "type": "rust",
          "style": "diamond",
          "foreground": "#000000",
          "background": "#DEA584",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\ue7a8 {{.Full}}"
        },
        {
          "type": "csharp",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#68217A",
          "leading_diamond": "",
          "trailing_diamond": "\ue0b0",
          "template": "\uf81a {{.Full}}"
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#33DD33",
          "background_templates": ["{{ if gt .Code 0 }}#D32F2F{{ end }}"],
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#888888",
          "background": "transparent",
          "template": "\uf252 {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#888888",
          "background": "transparent",
          "template": "{{ if .Charging }}AC {{ end }}{{ .Icon }}{{ .Percentage }}% ",
          "properties": {
            "charged_icon": "\uf00c ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": ""
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#888888",
          "background": "transparent",
          "template": "\uf017 {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#D32F2F",
          "template": "\uf0e7"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#00C853",
          "foreground_templates": ["{{ if .Root }}#D32F2F{{ end }}"],
          "template": " \u276f "
        }
      ]
    }
  ]
}
